import GEOparse
import pandas as pd

# Download RNA-seq dataset from GEO
# Example dataset â€” you can change later
gse = GEOparse.get_GEO(geo="GSE183947", destdir="../data")

# Extract expression matrix
samples = []
for gsm_name, gsm in gse.gsms.items():
    samples.append(gsm.table)

expression = pd.concat(samples, axis=1)
expression.to_csv("../data/raw_expression.csv")

